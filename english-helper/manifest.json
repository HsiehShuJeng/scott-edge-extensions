{
    "manifest_version": 3,
    "name": "Asking Expert",
    "version": "2.3.5",
    "description": "An asking helper to generate a question as prompt for Vocabulary.com, Duolingo, and programming efforts.",
    "icons": {
        "48": "images/icon48.png"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
        }
    ],
    "permissions": [
        "clipboardWrite",
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    }
}